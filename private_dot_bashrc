# Load all commons
if [ -f ~/.commonrc ]; then
  source ~/.commonrc
fi

# Turn on advanced bash completion if the file exists
if [ -f $(brew --prefix)/etc/bash_completion ]; then
    . $(brew --prefix)/etc/bash_completion
fi

[ -f ~/.fzf.bash ] && source ~/.fzf.bash

. $(brew --prefix asdf)/asdf.sh
